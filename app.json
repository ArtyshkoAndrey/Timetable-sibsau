{
  "expo": {
    "name": "Timetable Sibsau",
    "slug": "timetable-sibsau",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.2.5",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/images/icon.png",
    "androidStatusBarColor": "#006CB5",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#006CB5"
    },
    "splash": {
      "backgroundColor": "#ffffff",
      "mdpi": "./assets /images/splash-port-mdpi.png",
      "hdpi": "./assets/images/splash-port-hdpi.png",
      "xhdpi": "./assets/images/splash-port-xhdpi.png",
      "xxhdpi": "./assets/images/splash-port-xxhdpi.png",
      "xxxhdpi": "./assets/images/splash-port-xxxhdpi.png",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fulliton.timetablesibsau"
    },
    "android": {
      "package": "com.fulliton.timetablesibsau",
      "splash": {
	      "backgroundColor": "#ffffff",
	      "mdpi": "./assets/images/splash-port-mdpi.png",
	      "hdpi": "./assets/images/splash-port-hdpi.png",
	      "xhdpi": "./assets/images/splash-port-xhdpi.png",
	      "xxhdpi": "./assets/images/splash-port-xxhdpi.png",
	      "xxxhdpi": "./assets/images/splash-port-xxxhdpi.png",
	      "resizeMode": "contain"
	    },
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    }
  }
}
